$contentCatalog = ptronicsContentCatalog
$contentCatalogName = PTronics Content Catalog
$contentCV = catalogVersion(CatalogVersion.catalog(Catalog.id[default = $contentCatalog]), CatalogVersion.version[default = Staged])[default = $contentCatalog:Staged]
$productCatalog = ptronicsProductCatalog
$productCatalogName = PTronics Product Catalog
$productCV = catalogVersion(catalog(id[default = $productCatalog]), version[default = 'Staged'])[unique = true, default = $productCatalog:Staged]
$picture = media(code, $contentCV);
$image = image(code, $contentCV);
$media = media(code, $contentCV);
$page = page(uid, $contentCV);
$contentPage = contentPage(uid, $contentCV);
$product = product(code, $productCV)
$category = category(code, $productCV)
$siteResource = jar:com.hybris.ptronics.initialdata.constants.PTronicsInitialDataConstants&/ptronicsinitialdata/import/sampledata/contentCatalogs/$contentCatalog
$productResource = jar:com.hybris.ptronics.initialdata.constants.PTronicsInitialDataConstants&/ptronicsinitialdata/import/sampledata/productCatalogs/$productCatalog
$jarResourceCms = jar:com.hybris.ptronics.initialdata.constants.PTronicsInitialDataConstants&/ptronicsinitialdata/import/sampledata/cockpits/cmscockpit
$addonExtensionName = b2bacceleratoraddon
$medias = medias(code, $contentCV);
$mediaContainer = media(qualifier, $contentCV);
$siteUid = ptronics
$jarResourceCmsImages = jar:com.hybris.ptronics.initialdata.constants.PTronicsInitialDataConstants&/ptronicsinitialdata/import/sampledata/cockpits/cmscockpit

$addonExtensionName = assistedservicestorefront

